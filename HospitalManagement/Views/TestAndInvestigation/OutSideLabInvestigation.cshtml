@section scripts{
    <script src="~/wwwroot/assets/js/date-eu.js"></script>
    <script type="text/javascript">
        //$(".InvestigationStation").addClass("subdrop");
        //$(".Submenu_InvestigationStation").css("display", "block");
        //$(".Submenu_InvestigationStation").css("display", "block");
        //$(".outSideLabInvestigation").addClass("active");

        var htmlstr = "";
        $(document).ready(function () {
            $("#Doctor").select2();
            $('#example1').DataTable({
                "paging": true,
                "order": [[2, "desc"]],
                "scrollCollapse": true,
                "searching": true,
                "columnDefs": [{ "targets": 2, "type": "date-eu" }],
                "bInfo": true
            });

        });
        $("#Doctor").select2();

        function showModal() {
            debugger;
            $('#InvestigationReport_model').modal('show');
        }

        function showModalOP() {
            debugger;
            $('#InvestigationReportOP_model').modal('show');
        }
        function SuccessMethod(data) {
            debugger;
            if (data.msg == "1") {

                var id = data.Id;
                var appointmentcode = id.split("$")
                var id = appointmentcode[0] + "" + appointmentcode[1];
                var id1 = appointmentcode[1] + "" + appointmentcode[0] + "" + appointmentcode[1];

                $("#" + id + "").remove();
                $("#" + id + "").html("<span class='custom-badge status-blue'>Uploaded</span >");
                $("#" + id1 + "").remove();
                $("#" + id1 + "").html("<span class='custom-badge status-green'>Success</span>");

                $('#cardboxinvreport').removeAttr('hidden');
                htmlstr = "<div class='alert alert-success alert-dismissible fade show' role='alert'> <strong>Success!</strong>  <span id='statusmsg'>Record Added Successfully!</span> <button type='button' class='close' data-dismiss='alert' aria-label='Close'><span aria-hidden='true'>&times;</span></button></div > ";
            }
            else if (data.msg == "0") {
                $('#cardboxinvreport').removeAttr('hidden');
                htmlstr = "<div class='alert alert-danger alert-dismissible fade show' role='alert'> <strong>Error!</strong> problem   has been occurred while submitting your data :<span id='statusmsg'>Record Not Saved!</span> <button type='button' class='close' data-dismiss='alert' aria-label='Close'><span aria-hidden='true'>&times;</span></button></div > ";
            }
            else if (data.msg == "2") {
                $('#cardboxinvreport').removeAttr('hidden');
                htmlstr = "<div class='alert alert-info alert-dismissible fade show' role='alert'>  <span id='statusmsg'>Please Fill Data!</span> <button type='button' class='close' data-dismiss='alert' aria-label='Close'><span aria-hidden='true'>&times;</span></button></div > ";
            }
