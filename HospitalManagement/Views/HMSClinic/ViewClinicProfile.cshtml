@model HospitalManagement.Models.CliniqModel

@{
    ViewBag.Title = "ViewClinicProfile";
}

<div class="content">
    <div class="row">
        <div class="col-md-6 col-12">
            <h4 class="page-title">My Profile</h4>
        </div>

        <div class="col-md-6 col-12 text-md-right m-b-30">
            <a href="~/HMSClinic/EditClinicProfile" class="btn btn-sm skin-gradient-btn"><i class="fa fa-plus"></i> Edit Profile</a>
        </div>
    </div>

    <div class="card-box profile-header card-topline-skin">
        <div class="row">
            <div class="col-md-12">
                <div class="profile-view">
                    <div class="profile-basic">
                        <div class="row">
                            <div class="col-md-5">
                                <div class="profile-img-wrap">
                                    <div class="profile-img">
                                        @if (Model.cliniclogo == "" || Model.cliniclogo == null)
                                        {
                                            var im = System.Configuration.ConfigurationManager.AppSettings["gethosdefaultimg"] + "default_hplogo.png";
                                            <img id="blah" alt="your image" class="" src="@im">
                                        }
                                        else
                                        {
                                            @Html.HiddenFor(model => model.cliniclogo, new { @id = "Set_Logo" })
                                            var img = System.Configuration.ConfigurationManager.AppSettings["gethosimgUrl"] + Model.cliniclogo;
                                            <img id="blah" class="" alt="your image" src="@img">
                                        }

                                        @*<a href="#"><img class="avatar" src="~/wwwroot/assets/img/Hoslogo.png" width="50" height="50" alt=""></a>*@
                                    </div>
                                </div>
                                <div class="profile-info-left">
                                    <h3 class="user-name m-t-0 mb-0">@Model.CliniqName </h3>
                                    @*<small class="text-muted">Gynecologist</small>*@
                                    <div class="staff-id">Clinic Code : @Model.CliniqCode </div>
                                    @*<div class="staff-msg"><a href="chat.html" class="btn btn-primary">Send Message</a></div>*@
                                </div>
                            </div>
                            <div class="col-md-7">
                                <ul class="personal-info">
                                    <li>
                                        <span class="title">Phone  </span>
                                        <span class="text"> : @Model.Cliniq_phone </span>
                                    </li>
                                    <li>
                                        <span class="title">Email</span>
                                        <span class="text"><a href="#"> : @Model.Cliniq_email </a></span>
                                    </li>
                                    <li>
                                        <span class="title">Ragistration No.</span>
                                        <span class="text"> : @Model.Cliniq_RegNo </span>
                                    </li>
                                    <li>
                                        <span class="title">Address</span>
                                        <span class="text"> : @Model.Cliniq_Addr </span>
                                    </li>
                                    @*<li>
                                            <span class="title">Gender:</span>
                                            <span class="text">Female</span>
                                        </li>*@
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row mt-3">
        <div class="col-md-12">
            <div class="inv_tab">
                <ul class="nav nav-tabs nav-tabs-bottom">
                    <li class="nav-item"><a class="nav-link active" href="#about-cont" data-toggle="tab">Timing</a></li>
                    @*<li class="nav-item"><a class="nav-link" href="#bottom-tab2" data-toggle="tab">Profile</a></li>
